UPDATE PCEST SET 
       CUSTOULTENT = (SELECT (CUSTOULTENT )  FROM PCEST WHERE CODFILIAL = 1 AND CODPROD = &PRODANTIGO),
       CUSTOCONT = (SELECT (CUSTOCONT ) FROM PCEST WHERE CODFILIAL = 1 AND CODPROD = &PRODANTIGO), 
       CUSTOREAL = (SELECT (CUSTOREAL ) FROM PCEST WHERE CODFILIAL = 1 AND CODPROD = &PRODANTIGO), 
       CUSTOFIN  = (SELECT (CUSTOFIN )  FROM PCEST WHERE CODFILIAL = 1 AND CODPROD = &PRODANTIGO),
       CUSTOREP  = (SELECT (CUSTOREP  ) FROM PCEST WHERE CODFILIAL = 1 AND CODPROD = &PRODANTIGO),
       VALORULTENT = (SELECT (VALORULTENT ) FROM PCEST WHERE CODFILIAL = 1 AND CODPROD = &PRODANTIGO),
       CUSTOREALSEMST = (SELECT (CUSTOREALSEMST ) FROM PCEST WHERE CODFILIAL = 1 AND CODPROD = &PRODANTIGO), 
       CUSTOULTPEDCOMPRA = (SELECT (CUSTOULTPEDCOMPRA )  FROM PCEST WHERE CODFILIAL = 1 AND CODPROD = &PRODANTIGO),
       CUSTOULTENTFIN = (SELECT (CUSTOULTENTFIN )  FROM PCEST WHERE CODFILIAL = 1 AND CODPROD = &PRODANTIGO),
       VLULTENTCONTSEMST = (SELECT (VLULTENTCONTSEMST ) FROM PCEST WHERE CODFILIAL = 1 AND CODPROD = &PRODANTIGO), 
       CUSTOULTENTLIQ = (SELECT (CUSTOULTENTLIQ )  FROM PCEST WHERE CODFILIAL = 1 AND CODPROD = &PRODANTIGO),
       CUSTOREALLIQ = (SELECT (CUSTOREALLIQ ) FROM PCEST WHERE CODFILIAL = 1 AND CODPROD = &PRODANTIGO),
       VLULTPCOMPRA = (SELECT (VLULTPCOMPRA )  FROM PCEST WHERE CODFILIAL = 1 AND CODPROD = &PRODANTIGO), 
       BASEICMSULTENT = (SELECT (BASEICMSULTENT ) FROM PCEST WHERE CODFILIAL = 1 AND CODPROD = &PRODANTIGO),
       CUSTOULTENTCONT = (SELECT (CUSTOULTENTCONT ) FROM PCEST WHERE CODFILIAL = 1 AND CODPROD = &PRODANTIGO), 
       BASEICMSULTENTTAB = (SELECT (BASEICMSULTENTTAB ) FROM PCEST WHERE CODFILIAL = 1 AND CODPROD = &PRODANTIGO), 
       CUSTOFINSEMST = (SELECT (CUSTOFINSEMST ) FROM PCEST WHERE CODFILIAL = 1 AND CODPROD = &PRODANTIGO), 
       CUSTOULTENTFINSEMST = (SELECT (CUSTOULTENTFINSEMST ) FROM PCEST WHERE CODFILIAL = 1 AND CODPROD = &PRODANTIGO),
       CUSTOULTENTSEMST = (SELECT (CUSTOULTENTSEMST ) FROM PCEST WHERE CODFILIAL = 1 AND CODPROD = &PRODANTIGO), 
       CUSTOULTPEDCOMPRASEMST = (SELECT (CUSTOULTPEDCOMPRASEMST ) FROM PCEST WHERE CODFILIAL = 1 AND CODPROD = &PRODANTIGO), 
       VLICMSBCR = (SELECT (VLICMSBCR )  FROM PCEST WHERE CODFILIAL = 1 AND CODPROD = &PRODANTIGO), 
       BASEICMSBCR = (SELECT (BASEICMSBCR ) FROM PCEST WHERE CODFILIAL = 1 AND CODPROD = &PRODANTIGO)
WHERE CODFILIAL = 1 AND CODPROD = &PRODNOVO;
-------------------------------
SELECT CODPROD, CUSTOCONT, CUSTOREAL, CUSTOFIN, CUSTOREP, VALORULTENT, CUSTOREALSEMST, CUSTOULTPEDCOMPRA, CUSTOULTENTFIN, VLULTENTCONTSEMST, 
       CUSTOULTENTLIQ, CUSTOREALLIQ, VLULTPCOMPRA, BASEICMSULTENT, CUSTOULTENTCONT, BASEICMSULTENTTAB, CUSTOFINSEMST, CUSTOULTENTFINSEMST,
       CUSTOULTENTSEMST, CUSTOULTPEDCOMPRASEMST, VLICMSBCR, BASEICMSBCR
FROM PCEST WHERE CODPROD in (:CODPROD) AND CODFILIAL = 1;
