select rowid, p.codprod, p.classe, p.pcomint1, p.pcomrep1, p.pcomext1 from pcprodut p where codprod = &CODPROD;

select rowid, p.codprod, p.classe, p.pcomrep1, p.pcomint1, p.pcomext1 from pcprodfilial p where codprod = &CODPROD;

SELECT P.CODPROD, P.DESCRICAO, E.QTESTGER, P.CLASSE AS CLASSEP, F.CLASSE AS CLASSEF, 
P.PCOMINT1 AS COM1P, F.PCOMINT1 AS COM1F, P.PCOMREP1 AS PCOMREP1P, F.PCOMREP1 AS PCOMREP1F,
P.PCOMEXT1 AS PCOMEXT1P, F.PCOMEXT1 AS PCOMEXT1F
FROM PCPRODUT P, PCPRODFILIAL F, PCEST E
WHERE P.CLASSE <> F.CLASSE
AND F.CODFILIAL = 1
AND P.CODPROD = F.CODPROD
AND P.CODPROD = E.CODPROD
AND E.CODFILIAL = 1
AND E.QTESTGER > 0
AND F.PCOMINT1 IS NOT NULL;

SELECT CODPROD, QT, PVENDA, PERCOM 
FROM PCPEDI 
WHERE DATA BETWEEN '01/12/2021' AND '26/01/2022'
AND CODUSUR = &RCA 
AND CODPROD IN (  ); 


